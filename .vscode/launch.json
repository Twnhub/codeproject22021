    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "enter program name, for example ${workspaceFolder}/a.exe",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "miDebuggerPath": "/path/to/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Open indext002.html",
            "file": "c:\\Users\\user\\OneDrive\\桌面\\codeproject22021\\indext002.html"
        }
    ]
}